          "value": {
            "type": "text"
          }
        },
        "type": "object"
      }
    }
  },
  "settings": {
    "analysis": {
      "analyzer": {
        "recid_analyzer": {
          "tokenizer": "recid_tokenizer"
        }
      },
      "normalizer": {
        "lowercase_normalizer": {
          "type": "custom",
          "char_filter": [],
          "filter": [
            "lowercase"
          ]
        }
      },
      "tokenizer": {
        "recid_tokenizer": {
          "type": "pattern",
          "pattern": "(\\d+)",
          "group": 1
        }
      }
    }
  }
